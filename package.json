{
  "name": "unrarbg",
  "version": "1.0.2",
  "description": "a tool to move media files from rarbg",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint src --fix --ext .ts",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -e",
    "prepublishOnly": "npm run build",
    "test": "npm run lint",
    "type-check": "tsc --noEmit",
    "build": "rimraf lib && npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noamokman/unrarbg.git"
  },
  "keywords": [
    "rarbg"
  ],
  "author": {
    "name": "Noam Okman",
    "email": "noamokman@gmail.com",
    "url": "https://github.com/noamokman"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/noamokman/unrarbg/issues"
  },
  "homepage": "https://github.com/noamokman/unrarbg#readme",
  "module": "src/index.ts",
  "engines": {
    "node": ">=10"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-angular": "^8.2.0",
    "@types/pify": "^3.0.2",
    "@types/rimraf": "^2.0.2",
    "@types/update-notifier": "^2.5.0",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "babel-core": "^7.0.0-bridge.0",
    "eslint": "^6.5.1",
    "eslint-config-noamokman": "^9.1.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.19.0",
    "eslint-plugin-lodash": "^6.0.0",
    "eslint-plugin-unicorn": "^12.1.0",
    "husky": "^3.0.8",
    "lint-staged": "^9.4.2",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "caporal": "^1.3.0",
    "p-filter": "^2.1.0",
    "p-map": "^3.0.0",
    "p-pipe": "^3.0.0",
    "p-reflect": "^2.1.0",
    "pify": "^4.0.1",
    "read-pkg": "^5.2.0",
    "rimraf": "^3.0.0",
    "update-notifier": "^3.0.1"
  },
  "bin": {
    "unrarbg": "bin/unrarbg"
  }
}
