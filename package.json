{
  "name": "unrarbg",
  "version": "1.0.2",
  "description": "a tool to move media files from rarbg",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint src --fix",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -e",
    "babel": "babel src -d lib -s",
    "build": "rimraf lib && npm run babel",
    "prepublishOnly": "npm run build",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noamokman/unrarbg.git"
  },
  "keywords": [
    "rarbg"
  ],
  "author": {
    "name": "Noam Okman",
    "email": "noamokman@gmail.com",
    "url": "https://github.com/noamokman"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/noamokman/unrarbg/issues"
  },
  "homepage": "https://github.com/noamokman/unrarbg#readme",
  "module": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@commitlint/cli": "^7.5.0",
    "@commitlint/config-angular": "^7.5.0",
    "babel-core": "^7.0.0-bridge.0",
    "eslint": "^5.13.0",
    "eslint-config-noamokman": "^8.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.2.2",
    "eslint-plugin-lodash": "^5.1.0",
    "eslint-plugin-unicorn": "^7.1.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.2"
  },
  "dependencies": {
    "caporal": "^1.1.0",
    "p-filter": "^1.0.0",
    "p-map": "^2.0.0",
    "p-pipe": "^1.2.0",
    "p-reflect": "^1.0.0",
    "pify": "^4.0.1",
    "rimraf": "^2.6.3",
    "update-notifier": "^2.5.0"
  },
  "bin": {
    "unrarbg": "bin/unrarbg"
  }
}
